{
  "name": "blogger",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --port 3001",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@pixi/sound": "^4.2.0",
    "@smui/button": "^6.0.0-beta.16",
    "@smui/checkbox": "^6.0.0-beta.16",
    "@smui/dialog": "^6.0.0-beta.16",
    "@smui/form-field": "^6.0.0-beta.16",
    "@smui/icon-button": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.16",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.1",
    "dayjs": "^1.10.7",
    "elizabot": "^0.0.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-fetch": "^3.2.4",
    "pixi-text-input": "^1.0.6",
    "pixi.js": "^6.3.2",
    "pixi.js-keyboard": "^1.1.6",
    "qrcode": "^1.5.0",
    "smui-theme": "^6.0.0-beta.16",
    "suncalc": "^1.9.0",
    "svelte": "^3.47.0",
    "svelte-calendar": "^3.1.6",
    "svelte-check": "^2.6.0",
    "svelte-preprocess": "^4.9.4",
    "svelte-tiny-linked-charts": "^1.1.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1"
  }
}
